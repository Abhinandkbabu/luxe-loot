<script>
  // Function to remove the div after 2 seconds
  function removeDiv() {
      var divToRemove = document.getElementById('remove-alert');
      if (divToRemove) {
          setTimeout(function() {
              divToRemove.remove();
          }, 2000);
      }
  }
  // Call the removeDiv function to start the process
  removeDiv();
</script>
<script>
    const navTabs = document.querySelectorAll("#nav-tabs > a");
navTabs.forEach((tab) => {
  tab.addEventListener("click", () => {
    navTabs.forEach((tab) => {
      tab.classList.remove("active");
    });
    tab.classList.add("active");
  });
});
</script>
